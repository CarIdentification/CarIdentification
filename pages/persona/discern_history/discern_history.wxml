<view wx:for="{{historyInfos}}">
  <view class="cu-card">
    <view class="cu-item shadow-lg">
      <view class="image cover-pic">
        <image src="https://www.kebencool.cn:8762{{item.pic1}}"></image>
      </view>
      <view class="text-sm flex justify-end padding-right-sm">
        识别时间：{{item.createTime}}
      </view>
    </view>
  </view> 
</view>
<view class="page-flex-row" wx:if="{{historyInfos.length==0}}" style='margin-top: 100px;margin-bottom: 120rpx;'>
      <view class='page-flex-column'>
        <image style='width:250rpx;height:250rpx' src='/resource/image/not_result.png'></image>
        <text>你没搜索过哦...</text>
      </view>

    </view>