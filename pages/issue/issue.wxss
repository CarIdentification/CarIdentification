page{
  display: flex;
  flex-direction: column;
  height: 100%
}


.main {
    margin:0;
}
.navbar{
  flex: none;
  display: flex;
  background-color: white;
}
.navbar .item{
  flex: auto;
  text-align: center;
  position: relative;
  line-height: 50px;
  border: 0px solid black;
  margin: 1px;
  font-size: 15px;
}
.navbar .item.active{
  color: #FFCC00
}


.searchbar-result{
    width: 100%;
    margin-top: 0rpx;
    font-size:small;
    z-index:100;

}
.searchbar-result:before{
    display: none;
}
.fixed{
  position:fixed;
  width:100%;
  height:96rpx;
  z-index:100;

}