.search {
  width: 100vw;
  background-color: rgba(197, 212, 250, 1);
  position: fixed;
  color: white;
  top: 91vh;
}

.weui-grid__icon {
  display: block;
  width: 56px;
  height: 56px;
  margin: 0 auto;
}

.weui-grids_level {
  overflow: hidden;
}

.weui-grid_level {
  position: relative;
  float: left;
  padding: 20px 10px;
  width: 33.33333333%;
  box-sizing: border-box;
}
