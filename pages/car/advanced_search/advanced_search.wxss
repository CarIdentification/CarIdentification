.price-input {
  border: 1px solid #efeff4;
  /* width: 15%; */
  width: 20vw;
  border-radius: 5px;
  margin: 20rpx 20rpx 20rpx 20rpx;
}

.text {
  font-size: 14px;
  line-height: 350%;
}

.text-title-view {
  font-size: 15px;
  border-bottom-width: 2rpx;
  border-bottom-style: solid;
  border-bottom-color: #efeff4;
}

.text-title {
  margin: 15rpx 25rpx 15rpx 25rpx;
  font-size: 16px;
  color: black;
  align-items: center;
  justify-content: center;
}

.square_label {
  font-size: 12px;
  border-radius: 4px;
  padding: 2vw 0vw;
  margin: 1px;
  border: 1px solid;
  border-color: rgba(197, 212, 250, 0);
  width: 23vw;
  text-align: center;
}

.square {
  border-color: rgba(197, 212, 250, 0);
  text-align: center;
}

.is_checked {
  color: white;
  background: rgba(197, 212, 250, 1);
}

view {
  font-size: 12px;
  color: grey;
}

